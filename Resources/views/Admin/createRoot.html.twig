{% extends '::base.html.twig' %}

{% block body %}
    <h1>Create a new Menu</h1>
    <div>
        {{ form_start(form) }}
        {{ form_row(form.title) }}
        <a href="{{ path('matuckmenu') }}" class="btn btn-primary pull-right" role="button"><span class="glyphicon glyphicon-ban-circle"></span> Cancel</a>
        <button type="submit" value="Submit" class="btn btn-primary pull-right"><span class="glyphicon glyphicon-plus"> </span> Create</button>
        {{ form_end(form) }}
    </div>
{% endblock %}