{
    "name" : "matuck/menubundle",
    "description" : "A system provide menus, and an admin panel to manage them.",
    "type" : "symfony-bundle",
    "authors" : [{
        "name" : "matuck",
        "email" : "matuck@matuck.com"
    }],
    "keywords" : [
        "menu nav-bar navbar"
    ],
    "license" : [
        "MIT"
    ],
    "require" : {
	"stof/doctrine-extensions-bundle": "^1.2",
	"knplabs/knp-menu-bundle": "^2.1"
    },
    "autoload" : {
        "psr-0" : {
            "matuck\\MenuBundle" : ""
        }
    },
    "target-dir" : "matuck/MenuBundle",
    "repositories" : [{
	"type": "vcs",
	"url": "https://github.com/matuck/MenuBundle.git"
    }],
    "extra" : {
    "branch-alias" : {
            "dev-master" : "master-dev"
        }
    }
}
